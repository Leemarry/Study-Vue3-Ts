{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,
    "types": ["vite/client"],
    "baseUrl": ".",
    "paths":{
      "@/*":["src/*"] // 
      },
  },
  // "include": ["vite.config.ts"],
  "include": ["vite.config.ts","src/**/*.ts",  "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue", "antd.ts",],

}
